{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "prefer-const": true,
        "prefer-conditional-expression": true,
        "semicolon": true,
        "ordered-imports": true,
        "quotemark": false,
        "only-arrow-functions": false,
        "space-before-function-paren": false,
        "no-console": false,
        "max-line-length": [
            true,
            130
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "**/__tests__/**"
        ]
    }
}