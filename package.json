{
  "name": "lib-of-commons",
  "version": "0.1.3",
  "description": "A Utility Library for Typescript that offers a variety of Decorators. That solve common tasks, hence the name Lib of Commons, like logging or parameter validation and many more.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "check:linter": "tslint -p .",
    "check:security": "npm audit --only=prod",
    "fix:linter": "tslint -p . --fix",
    "fix:security": "npm audit fix --only=prod",
    "test": "jest --coverage",
    "prepublishOnly": "npm run test && npm run build"
  },
  "license": "MIT",
  "repository": {
    "type": "Github",
    "url": "https://github.com/Templum/lib-of-commons"
  },
  "homepage": "https://github.com/Templum/lib-of-commons",
  "bugs": {
    "url": "https://github.com/Templum/lib-of-commons/issues/new?assignees=&labels=Bug&template=bug-report.md&title=%5BBUG%5D+-+Start+here+with+a+descriptive+Title"
  },
  "author": "Simon Pelczer <templum.dev@gmail.com>",
  "contributors": [
    {
      "name": "Simon Pelczer",
      "email": "templum.dev@gmail.com"
    }
  ],
  "dependencies": {
    "reflect-metadata": "0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "10.0.3",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "typescript": "3.4.4"
  },
  "files": [
    "/dist",
    "README.md",
    "LICENSE"
  ]
}